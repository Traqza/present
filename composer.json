{
    "name": "traqza/present",
    "description": "Create presenters for Eloquent Models",
    "keywords": [
        "presentable"
    ],
    "homepage": "https://github.com/Traqza/present",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Traqza\\Presentable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Traqza\\Presentable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Traqza\\Presentable\\PresentableServiceProvider"
            ]
        }
    }
}
